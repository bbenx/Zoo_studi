{% extends 'base.html.twig' %}

{% block title %}Horaires
{% endblock %}

{% block body %}
	<h1>Horaires</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ horaire.id }}</td>
			</tr>
			<tr>
				<th>Lundi</th>
				<td>{{ horaire.Lundi }}</td>
			</tr>
			<tr>
				<th>Mardi</th>
				<td>{{ horaire.Mardi }}</td>
			</tr>
			<tr>
				<th>Mercredi</th>
				<td>{{ horaire.Mercredi }}</td>
			</tr>
			<tr>
				<th>Jeudi</th>
				<td>{{ horaire.Jeudi }}</td>
			</tr>
			<tr>
				<th>Vendredi</th>
				<td>{{ horaire.Vendredi }}</td>
			</tr>
			<tr>
				<th>Samedi</th>
				<td>{{ horaire.Samedi }}</td>
			</tr>
			<tr>
				<th>Dimanche</th>
				<td>{{ horaire.Dimanche }}</td>
			</tr>
			<tr>
				<th>CreationDate</th>
				<td>{{ horaire.creationDate |date('Y-m-d H:i:s') }}</td>
			</tr>
			<tr>
				<th>ModificationDate</th>
				<td>{{ horaire.modificationDate ? horaire.modificationDate|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('app_horaires_index') }}">back to list</a>

	<a href="{{ path('app_horaires_edit', {'id': horaire.id}) }}">edit</a>

	{{ include('horaires/_delete_form.html.twig') }}
{% endblock %}
